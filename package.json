{
  "name": "autogent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node -r dotenv/config ./scripts/migrate.js",
    "db:init": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/init-db.ts",
    "db:seed": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/seed-db.ts",
    "db:check": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/check-db.ts",
    "db:connection": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/get-connection-string.ts",
    "db:studio": "drizzle-kit studio",
    "test-agent": "ts-node --esm scripts/test-agent.ts",
    "test-results-scraper": "tsx -r dotenv/config ./scripts/test-results-scraper.ts",
    "test-active-scraper": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/test-active-scraper.ts",
    "upload-to-supabase": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/upload-to-supabase.ts",
    "upload-completed": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/upload-to-supabase.ts --completed",
    "upload-active": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/upload-to-supabase.ts --active",
    "create-supabase-tables": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/create-supabase-tables.ts",
    "create-supabase-tables-direct": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/create-supabase-tables-direct.ts",
    "test-supabase": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/test-supabase-connection.ts",
    "check-supabase-url": "ts-node --esm -r dotenv/config --project ./scripts/tsconfig.json ./scripts/check-supabase-url.ts"
  },
  "dependencies": {
    "@langchain/community": "^0.3.32",
    "@langchain/core": "^0.3.41",
    "@langchain/openai": "^0.4.4",
    "@supabase/supabase-js": "^2.49.1",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
