    "langchain": "^0.3.19",
    "lucide-react": "^0.476.0",
    "next": "15.2.0-canary.76",
    "pg": "^8.13.3",
    "postgres": "^3.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "vega": "^5.32.0",
    "vega-lite": "^5.23.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20",
    "@types/pg": "^8.11.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "buffer": "^6.0.3",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9",
    "eslint-config-next": "15.2.0-canary.76",
    "process": "^0.11.10",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5"
  },
